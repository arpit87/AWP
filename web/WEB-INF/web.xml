<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>adminLoginProcess</servlet-name>
        <servlet-class>Transport.adminLoginProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contactus</servlet-name>
        <servlet-class>Transport.contactus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createNewManager</servlet-name>
        <servlet-class>Transport.createNewManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>installNewTrucks</servlet-name>
        <servlet-class>Transport.installNewTrucks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Transport.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchTruck</servlet-name>
        <servlet-class>Transport.searchTruck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Transport.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateManager</servlet-name>
        <servlet-class>Transport.updateManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changeStatusOfTruck</servlet-name>
        <servlet-class>Transport.changeStatusOfTruck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminLoginProcess</servlet-name>
        <url-pattern>/adminLoginProcess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contactus</servlet-name>
        <url-pattern>/contactus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createNewManager</servlet-name>
        <url-pattern>/createNewManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>installNewTrucks</servlet-name>
        <url-pattern>/installNewTrucks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchTruck</servlet-name>
        <url-pattern>/searchTruck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateManager</servlet-name>
        <url-pattern>/updateManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changeStatusOfTruck</servlet-name>
        <url-pattern>/changeStatusOfTruck</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
